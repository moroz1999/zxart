{
  "name": "moroz1999/zxart",
  "description": "ZX-Art",
  "require": {
    "artweb-ou/trickster-cms": "^5.1",
    "moroz1999/sxg": "*",
    "moroz1999/zx-image": "^5.4",
    "moroz1999/zx-files": "^3.1",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-dom": "*",
    "ext-zip": "*",
    "openai-php/client": "^0.7.3",
    "http-interop/http-factory-guzzle": "^1.2",
    "guzzlehttp/guzzle": "^7",
    "php-http/discovery": "^1.19",
    "smalot/pdfparser": "^2.7",
    "ext-fileinfo": "*",
    "verifalia/sdk": "^3.0",
    "php-di/php-di": "^7.0",
    "ext-libxml": "*",
    "ext-redis": "*",
    "ext-pdo": "*",
    "ezyang/htmlpurifier": "^4.19",
    "ext-xmlwriter": "*",
    "symfony/object-mapper": "^8.0"
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "php-http/discovery": true
    },
    "audit": {
      "block-insecure": false
    }
  },
  "require-dev": {
    "vimeo/psalm": "^6.13",
    "phpunit/phpunit": "^12",
    "dg/bypass-finals": "^1.4"
  },
  "scripts": {
    "test": "vendor\\bin\\phpunit -c phpunit.xml.dist",
    "psalm": "vendor\\bin\\psalm --use-baseline=psalm-baseline.xml --no-cache",
    "psalm-baseline": "vendor\\bin\\psalm --set-baseline=psalm-baseline.xml --no-cache",
    "build": "cd .\\ng-zxart && npm run build"
  }
}
